{
  "manifest_version": 3,
  "name": "tabs2text",
  "description": "Export list of open tabs as markdown / org text",
  "version": "0.3",
  "permissions": ["tabs", "downloads"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["service-worker.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tabs2text@mentat.za.net"
    }
  }
}
